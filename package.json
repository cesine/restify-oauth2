{
    "name": "restify-oauth2-oauthd",
    "description": "A simple OAuth 2 endpoint for Restify + OAuth.io modifications",
    "keywords": [
        "restify",
        "oauth",
        "oauthd",
        "oauth.io",
        "oauth2",
        "rest",
        "authentication",
        "api"
    ],
